pipeline{
agent any
 stages {
    stage('Sonar Analyse') {
      steps {
        script  {
                def scannerHome = tool 'SonarQubeScanner';
                withSonarQubeEnv('SonarQube') {
                  bat "${scannerHome}\bin\sonar-scanner"
                }
               }
      }
    }
 }
}
