pipeline{
 agent any
 environment {
    ANYPOINT = credentials('ANYPOINT')
	PATH = 'C:/WINDOWS/SYSTEM32'
 }
 stages {
 	stage ('Build'){
 		steps {
 			withMaven(maven:'maven'){
 				bat 'mvn -f mockhttprequest/pom.xml clean install'
 			}
 		}
 	}
 	stage ('Deploy'){
 		steps {
 			withMaven(maven:'maven'){
 				bat 'mvn deploy -DmuleDeploy -Danypoint.username=$ANYPOINT_USR -Danypoint.password=$ANYPOINT_PSW -Danypoint.environment=Sandbox'
 			}
 		}
 	}
 }

}