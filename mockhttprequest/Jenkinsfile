pipeline{
 agent any
 environment {
    ANYPOINT = credentials('ANYPOINT')
	PATH = 'C:/WINDOWS/SYSTEM32'
 }
 stages {
	stage('Unit') {
		steps {
			bat 'mvn clean test'
			}
		}
	stage('Deploy CloudHub') {
		steps {
			bat 'mvn deploy -DmuleDeploy -Danypoint.environment=Sandbox -DcloudhubAppName=mulehttprequest -Danypoint.username=$ANYPOINT_USR -Danypoint.password=$ANYPOINT_PSW'
		}
 		}
 	}
 }