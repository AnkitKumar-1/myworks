pipeline{
 agent any
 environment {
    ANYPOINT = credentials('ANYPOINT')
 }
 stages {
 	stage ('Build'){
 		steps {
 			withMaven(maven:'maven'){
 				sh 'mvn -f mockhttprequest/pom.xml clean install'
 			}
 		}
 	}
 	stage ('Deploy'){
 		steps {
 			withMaven(maven:'maven'){
 				sh 'mvn -f mockhttprequest/pom.xml package deploy -Danypoint.username=$ANYPOINT_USR -Danypoint.password=$ANYPOINT_PSW -Danypoint.environment=Sandbox -DmuleDeploy'
 			}
 		}
 	}
 }

}