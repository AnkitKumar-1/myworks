pipeline{
 agent any
 environment {
    ANYPOINT = credentials('ANYPOINT')
	PATH = 'C:/WINDOWS/SYSTEM32'
 }
 stages {
    stage('Unit Test') {
      steps {
      	withMaven(maven:'maven'){
       		bat 'mvn -f mockhttprequest/pom.xml clean test'
      	}
      }
    }
    stage ('Build'){
 		steps {
 			withMaven(maven:'maven'){
 				bat 'mvn -f mockhttprequest/pom.xml clean install'
 			}
 		}
 	}
 	stage ('Deploy CloudHub'){
 		steps {
 			withMaven(maven:'maven'){
 				bat 'mvn deploy -P cloudhub  -Dusername=vij4m3 -Dpassword=Ted@123'
 			}
 		}
 	}
 }

}