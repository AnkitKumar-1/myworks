pipeline{
agent any
 stages {
    stage('Sonar Analyse') {
      steps {
        script  {
                def scannerHome = tool 'SonarQubeScanner';
                withSonarQubeEnv('SonarQube') {
                  bat " -f project2/sonar-project.properties ${scannerHome}/bin/sonar-scanner"
                }
               }
      }
    }
 }
}
