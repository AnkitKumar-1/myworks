pipeline {
agent any
stages {
    stage('Sonar Analyse') {
      steps {
        script  {
              def scannerHome = tool 'SonarQubeScanner';
                withSonarQubeEnv('SonarQube') {
                  bat "mvn org.sonarsource.scanner.maven:sonar-maven-plugin:3.6.0.1398:sonar"
                }
               }
              } 
            }
        }
    }
