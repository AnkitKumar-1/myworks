pipeline {
agent any
stages {

    stage('Sonar Analyse') {
        steps {
            nodejs(nodeJSInstallationName: 'NodeJS', configId: '') {

                // sonar script moet je in een nodejs module draaien !!
                script {
                  withSonarQubeEnv('SonarQube') {
                    bat "sonar-scanner"
                }
            }
            }
        }
    }
}
}
