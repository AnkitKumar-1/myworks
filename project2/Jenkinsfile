pipeline{
 agent any
 environment {
    ANYPOINT = credentials('ANYPOINT')
	  PATH = 'C:/WINDOWS/SYSTEM32'
    scannerHome = tool 'SonarScanner'
 }
 stages {
	stage('SonarQube Analysis'){
    steps {
			withSonarQubeEnv('SonarQube'){
        bat "${scannerHome}/sonar-scanner"
			}
		  }
    }
 }

}
