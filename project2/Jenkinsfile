pipeline{
 agent any
 environment {
    ANYPOINT = credentials('ANYPOINT')
	  PATH = 'C:/WINDOWS/SYSTEM32'
 }
 stages {
	stage('SonarQube Analysis'){
    steps {
      script {
        def sonarqubeScannerHome = tool 'SonarScanner'
      }
			withSonarQubeEnv('SonarQube'){
				bat "${sonarqubeScannerHome}/bin/sonar-scanner"
			}
		  }
    }
 }

}
